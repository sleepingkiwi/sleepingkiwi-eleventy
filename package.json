{
  "name": "sleepingkiwi-eleventy",
  "version": "1.0.0",
  "description": "A starting point for building static sites with eleventy using netlify CMS for content",
  "keywords": [],
  "author": "sleepingkiwi",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleepingKiwi/sleepingkiwi-eleventy.git"
  },
  "bugs": {
    "url": "https://github.com/sleepingKiwi/sleepingkiwi-eleventy/issues"
  },
  "homepage": "https://github.com/sleepingKiwi/sleepingkiwi-eleventy#readme",
  "dependencies": {
    "colorthief": "^2.3.0",
    "core-js": "^3.4.0",
    "netlify-cms": "^2.9.7",
    "preact": "^10.0.5",
    "preact-router": "^3.1.0",
    "prop-types": "^15.7.2",
    "unfetch": "^4.1.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-react-jsx": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-modules": "^0.1.1",
    "autoprefixer": "^9.6.5",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cssnano": "^4.1.10",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "light-server": "2.6.4",
    "node-file-rev": "^1.1.2",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "source-map-loader": "^0.2.4",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.12.1",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  },
  "scripts": {
    "test": "echo \"For goodness sake. Write Some Tests.\" && exit 1",
    "clean": "rm -rf dist && rm -f src/_data/rev.json",
    "rev": "node-file-rev dist/admin.modules.js dist/main.js dist/main.modules.js dist/polyfills.js dist/styles/main.css --manifest=src/_data/rev.json --root=dist",
    "eleventy:default": "npx @11ty/eleventy",
    "eleventy:watch": "npx @11ty/eleventy --watch",
    "styles:lint": "stylelint src/styles/**/*.scss --color",
    "styles": "node-sass src/styles/ --output dist/styles/ --source-map true --source-map-contents true && postcss ./dist/styles/*.css --replace",
    "scripts:lint": "eslint src/js/**/*.js",
    "scripts:prod": "npm run scripts:lint && webpack --env.production --progress --color",
    "scripts:dev": "npm run scripts:lint && webpack --env.production=false --env.development --progress --color ",
    "scripts:stats": "webpack --profile --json --env.production > compilation-stats.json",
    "serve": "light-server -c .lightserverrc",
    "dev": "npm-run-all clean styles:lint styles scripts:dev eleventy:default --parallel eleventy:watch serve --print-label",
    "dev:debug": "DEBUG=* npm run dev",
    "start": "npm run dev",
    "build": "run-s clean styles:lint styles scripts:prod rev eleventy:default --print-label"
  }
}
